<blockquote class="instagram-media pat-embed"
            data-instgrm-captioned
            data-instgrm-version="7"
            data-pat-embed="src://platform.instagram.com/en_US/embeds.js;width:${width};height:${height}"
            tal:define="url view/data/url|nothing;
                        width view/data/width|nothing;
                        width python:width and str(width) or '100%';
                        height view/data/height|nothing;"
            tal:condition="url"><a href="${url}">Instagram</a></blockquote>
