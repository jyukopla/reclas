<header class="site-header site-header-${portal_name} container-fluid hidden-print" i18n:domain="plone"
        tal:define="portal_state context/@@plone_portal_state;
                    context_state context/@@plone_context_state;
                    navigation_root_url portal_state/navigation_root_url;
                    navigation_root_title portal_state/navigation_root_title;
                    portal_title portal_state/portal_title;
                    portal_object portal_state/portal;
                    portal_name portal_object/getId;
                    language portal_state/language;
                    title python:navigation_root_title or portal_title;
                    fi_root context/fi/absolute_url|nothing;
                    en_root context/en/absolute_url|nothing;
                    tg context/@@tg|nothing">
  <div class="site-menu-button-container container">
    <button class="site-menu-button">
    <span class="site-menu-button-icon">
      <span></span>
      <span></span>
      <span class="short"></span>
    </span>
    </button>
  </div>
  <div class="site-brand-container container">
    <div class="row">
      <div class="col-2 col-sm-2 col-md-10 col-xl-7 site-logo-container">
        <a class="site-logo" href="https://www.jyu.fi/en/"
             i18n:attributes="href"></a>
        <p class="hidden-sm-down site-name">
          <a href="https://www.jyu.fi/en/" i18n:translate="university_name"
               i18n:attributes="href">University of Jyväskylä</a>
          <tal:condition condition="python:portal_title != 'Jyväskylän yliopisto'.decode('utf-8')">
            <span class="separator">|</span>
            <a href="${navigation_root_url}" i18n:translate=""
                 tal:content="title">Faculty of Sport and Health Sciences</a>
          </tal:condition>
        </p>
      </div>
      <div class="col-7 col-sm-8 hidden-md-up site-name-container no-gutters"
           tal:condition="python:portal_title == 'Jyväskylän yliopisto'.decode('utf-8')">
        <p class="site-name site-name-sm">
          <a href="https://www.jyu.fi/en/" i18n:translate="university_name"
               i18n:attributes="href">University of Jyväskylä</a>
        </p>
      </div>
      <div class="col-7 col-sm-8 hidden-md-up site-name-container no-gutters site-name-animate"
           tal:condition="python:portal_title != 'Jyväskylän yliopisto'.decode('utf-8')">
        <p class="site-name site-name-sm">
          <a href="https://www.jyu.fi/en/" i18n:translate="university_name"
               i18n:attributes="href">University of Jyväskylä</a>
        </p>
        <p class="site-name site-name-sm">
          <a href="${navigation_root_url}" i18n:translate=""
               tal:content="title">
            Liikuntatieteellinen tiedekunta
          </a>
        </p>
      </div>
      <div class="hidden-lg-down col-xl-4 site-links-search-container">
        <div class="site-links-container">
          <span class="site-language-selector-container"
               tal:condition="python:fi_root or en_root">
            <a href="${navigation_root_url}/@@multilingual-selector/${tg}/fi" tal:condition="python:tg and language != 'fi'">FI</a>
            <a href="${navigation_root_url}/@@multilingual-selector/${tg}/en" tal:condition="python:tg and language != 'en'">EN</a>
            <a href="${fi_root}" tal:condition="python:not tg and fi_root and language != 'fi'">FI</a>
            <a href="${en_root}" tal:condition="python:not tg and en_root and language != 'en'">EN</a>
            <span class="separator">|</span>
          </span>
          <a href="https://www.jyu.fi/en/person-search"
               i18n:attributes="href" i18n:translate="">Contact search</a>
        </div>
        <div class="site-search-container"
             id="navbar-site-search" aria-hidden="true"
             data-pat-livesearch="itemTemplate:&lt;li class=&#34;search-result &lt;%- state %&gt;&#34;&gt;&lt;div class=&#34;title&#34;&gt;&lt;a href=&#34;&lt;%- url %&gt;&#34;&gt;&lt;%- title %&gt;&lt;/a&gt;&lt;/div&gt;&lt;div class=&#34;description&#34;&gt;&lt;%- description %&gt;&lt;/div&gt;&lt;/li&gt;;minimumInputLength:3">
          <form action="@@search" role="search" class="pat-livesearch"
                i18n:domain="plone"
                tal:define="folder_path python:'/'.join(context_state.folder().getPhysicalPath())"
                tal:attributes="action string:${navigation_root_url}/@@search;
                                data-pat-livesearch string:ajaxUrl:${navigation_root_url}/@@ajax-search">
            <div class="input-group">
              <label class="hiddenStructure invisible"
                     for="searchGadget" hidden="hidden"
                     i18n:translate="text_search">Search Site</label>

              <input name="SearchableText"
                     type="text"
                     size="18"
                     value=""
                     class="form-control searchGadget"
                     title="Search Site"
                     placeholder="Search current web pages"
                     i18n:attributes="title;placeholder;"
                     tal:attributes="value request/form/SearchableText|nothing" />

              <input class="invisible" hidden="hidden"
                     type="submit"
                     value="Search"
                     i18n:attributes="value label_search" />

            </div>
          </form>
        </div>
        <div class="site-search-toggle">
            <i class="fa fa-search fa-flip-horizontal pat-aria-expand"
               aria-controls="navbar-site-search" aria-expanded="false"></i>
        </div>
      </div>
    </div>
  </div>
  <div class="site-menu-container container-fluid">
    <div class="container">
